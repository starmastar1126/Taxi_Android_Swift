        <activity android:name="com.fastcabtaxi.driver.StaticPageActivity" />
        <activity android:name="com.fastcabtaxi.driver.ContactUsActivity" />
        <activity android:name="com.fastcabtaxi.driver.HelpActivity" />
        <activity android:name="com.fastcabtaxi.driver.QuestionAnswerActivity" />
        <activity
