        <activity android:name="com.twitter.sdk.android.core.identity.OAuthActivity" />
        <activity android:name="com.fastcabtaxi.driver.AppLoginActivity" />
        <activity android:name="com.fastcabtaxi.driver.SelectCountryActivity" />
